<table border="1" cellpadding="5">
  <thead>
    <tr>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
  {%- for user in users -%}
    <tr>
      <td><a href="/users/{{ user.id }}">{{ user.id }}</a></td>
      <td>{{ user.first_name }}</td>
      <td>{{ user.last_name }}</td>
      <td>{{ user.email }}</td>
    </tr>
  {%- endfor -%}
  </tbody>
</table>