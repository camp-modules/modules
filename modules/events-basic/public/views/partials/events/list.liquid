<table border="1" cellpadding="5">
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Start</th>
      <th>End</th>
    </tr>
  </thead>
  <tbody>
  {%- for event in events -%}
    <tr>
      <td><a href="/events-basic/events/{{ event.id }}">{{ event.id }}</a></td>
      <td>{{ event.properties.title }}</td>
      <td>{{ event.properties.start | date: '%m/%d/%Y (%H:%M)' }}</td>
      <td>{{ event.properties.end | date: '%m/%d/%Y (%H:%M)' }}</td>
    </tr>
  {%- endfor -%}
  </tbody>
</table>